<%= render 'errors' %>
<%= form_for @user do |user| %>
	<p>
		<%= user.label :name %> : <%= user.text_field :name %>
	</p>
	<p>
		<%= user.label :mail %>	: <%= user.email_field :mail %>		
	<p>
		<%= user.label :contact %> : <%= user.telephone_field :contact %>
	</p>
	<p>
		<%= user.label :user_type %> : <%= user.select :user_type, ['Admin', 'Land lord', 'Tenant'] %>
	</p>
	<p>
		<%= user.submit %>
	</p>
<% end %>